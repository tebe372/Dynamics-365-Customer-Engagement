{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md", "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
           "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "uhfHeaderId": "MSDocsHeader-Dynamics365"
    },
    "fileMetadata": {
      "breadcrumb_path": {
        "gamification/**.*": "bc/toc.yml",
        "voice-of-customer/**.*": "bc/toc.yml",
        "social-engagement/**.*": "bc/toc.yml",
        "portals/**.*": "bc/toc.yml",
        "linkedin/**.*": "bc/toc.yml",
        "sales-business/**.*": "bc/toc.yml"
      },
      "searchScope":{
        "voice-of-customer/**.*":["Voice of the Customer"],
        "gamification/**.*":["Gamification"],
        "social-engagement/**.*":["Social Engagement"],
        "portals/**.*":["Portals"],
        "linkedin/**.*":["LinkedIn"],
        "sales-business/**.*":["Sales-Business edition"]
      }
    },
    "template": [],
    "xref": ["_zip/dyn365-dotnet.zip"],
    "dest": "customer-engagement"
  }
}
