{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md", "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
           "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {},
    "fileMetadata": {
      "breadcrumb_path": {
        "gamification/**.*": "bc/toc.yml",
        "voice-of-customer/**.*": "bc/toc.yml",
        "social-engagement/**.*": "bc/toc.yml"
      }
    },
    "template": [],
    "dest": "customer-engagement"
  }
}
